{
   // SECTION Editor custom colours
   "editor.tokenColorCustomizations": {
      // NOTE Theme -FM mods
      "[Theme]": {
         "textMateRules": [
            {
               "name": "Meta punctuation definition tag",
               "scope": "punctuation.definition.tag", // <>
               "settings": {
                  "foreground": "#8298A6",
                  "fontStyle": ""
               }
            },
            {
               "name": "Meta tag and css puntuation separetor tag",
               "scope": [
                  "meta.tag", // =
                  "punctuation.separator.key-value.css", // :
                  "punctuation.separator.list.comma.css", // ,
                  "punctuation.terminator.rule.css" // ;
               ],
               "settings": {
                  "foreground": "#abb2bf",
                  "fontStyle": ""
               }
            },
            {
               "name": "Tag attribute",
               "scope": "entity.other.attribute-name",
               "settings": {
                  "foreground": "#37a7bb"
               }
            },
            {
               "name": "Entity other attribute name id",
               "scope": "entity.other.attribute-name.id",
               "settings": {
                  "foreground": "#85a5e9"
               }
            },
            {
               "name": "Entity other attribute name class",
               "scope": "entity.other.attribute-name.class",
               "settings": {
                  "foreground": "#bb7737"
               }
            },
            {
               "name": "Entity other attribute name pseudo-class",
               "scope": "entity.other.attribute-name.pseudo-class",
               "settings": {
                  "foreground": "#adff2f"
               }
            },
            {
               "name": "Entity other atrribute name pseudo-element",
               "scope": "entity.other.attribute-name.pseudo-element",
               "settings": {
                  "foreground": "#ffaf53"
               }
            },
            {
               "name": "Support function",
               "scope": "support.function",
               "settings": {
                  "foreground": "#3f9dd3"
               }
            },
            {
               "name": "String quoted",
               "scope": ["string.quoted", "variable.other.readwrite.js"],
               "settings": {
                  "foreground": "#d9b682"
               }
            },
            {
               "name": "CSS string double quoted",
               "scope": "string.quoted.double.css",
               "settings": {
                  "foreground": "#c76c6c"
               }
            },
            {
               "name": "CSS string single quoted",
               "scope": "string.quoted.single.css",
               "settings": {
                  "foreground": "#f832e6"
               }
            },
            {
               "name": "CSS other property value",
               "scope": "meta.property-value.css",
               "settings": {
                  "foreground": "#ff649b"
               }
            },
            {
               "name": "CSS support constant property value",
               "scope": "support.constant.property-value.css",
               "settings": {
                  "foreground": "#ff4678"
               }
            },
            {
               "name": "CSS support constant media",
               "scope": "support.constant.media.css",
               "settings": {
                  "foreground": "#0aa886" // 4972e0 blue-ish
               }
            },
            {
               "name": "Variable --(xyz)",
               "scope": "variable",
               "settings": {
                  "foreground": "#7fc76c"
               }
            }
         ]
      }
   },
   // SECTION Workbench custom colours
   "workbench.colorCustomizations": {
      // NOTE Theme -FM mods
      "[Theme]": {
         "editor.foreground": "#727781", // abb2bf
         "statusBar.background": "#084466",
         "statusBarItem.hoverBackground": "#0a527c"
      }
   }
}
